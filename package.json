{
  "name": "@linagora/linid-im-front-corelib",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/core-lib.umd.js",
  "module": "dist/core-lib.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "test": "vitest",
    "dev": "vite",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "type-check": "vue-tsc --noEmit",
    "validate": "pnpm type-check && pnpm lint && pnpm format:check"
  },
  "peerDependencies": {
    "pinia": "=3.0.3",
    "vue": "=3.5.22"
  },
  "devDependencies": {
    "@eslint/js": "=9.39.1",
    "@types/node": "=24.10.0",
    "@vitejs/plugin-vue": "=6.0.1",
    "@vue/eslint-config-prettier": "=10.2.0",
    "@vue/eslint-config-typescript": "=14.6.0",
    "eslint": "=9.39.1",
    "eslint-config-prettier": "=10.1.8",
    "eslint-plugin-jsdoc": "=61.2.0",
    "eslint-plugin-vue": "=10.5.1",
    "prettier": "=3.6.2",
    "typescript": "=5.9.3",
    "typescript-eslint": "=8.46.4",
    "vite": "=7.1.12",
    "vitest": "=4.0.7",
    "vue-eslint-parser": "=9.4.3",
    "vue-tsc": "=3.1.3"
  },
  "packageManager": "pnpm@10.20.0"
}
